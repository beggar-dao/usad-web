# Environment Configuration for GBPC Frontend
# Local Development Environment

# API Configuration (UMI_APP_ prefix required for client-side access)
# 使用代理路径，避免 CORS 问题
UMI_APP_API_BASE_URL=https://api.usad.vn.com
UMI_APP_API_TIMEOUT=10000

# Authentication Configuration (UMI_APP_ prefix required for client-side access)
UMI_APP_JWT_SECRET_KEY=your-jwt-secret-key-for-development
UMI_APP_JWT_EXPIRES_IN=3600
UMI_APP_REFRESH_TOKEN_EXPIRES_IN=604800

# Application Configuration (UMI_APP_ prefix required for client-side access)
UMI_APP_APP_ENV=development
UMI_APP_APP_NAME=GBPC
UMI_APP_APP_VERSION=1.0.0

# Debug Configuration (UMI_APP_ prefix required for client-side access)
UMI_APP_DEBUG_MODE=true
UMI_APP_LOG_LEVEL=debug

# Feature Flags (UMI_APP_ prefix required for client-side access)
UMI_APP_ENABLE_2FA=false
